{
    // 指定部署的目标项目
    "name": "nav-site-vue",
    // 边缘函数入口文件（实现后台管理API）
    "entry": "./src/edge-function.js",
    // 配置自定义安装指令
    "installCommand": "npm install",
    // 配置自定义构建命令
    "buildCommand": "npm run build",
    // 静态资源托管功能
    "assets": {
        // 构建产物中将被静态托管的目录
        "directory": "./dist",
        // 当请求的路径未匹配到任何静态资源时执行的策略
        "notFoundStrategy": "singlePageApplication"
    }
}
